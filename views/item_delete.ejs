<%- include('./head') %>
<h1><%= title %>: <%= item.name %></h1>
<p>Do you really want to delete this item?</p>
<form method="POST">
  <div class="form-group">
    <input type="hidden" id="itemid" name="itemid" class="form-control" value=<%= item._id%>>
